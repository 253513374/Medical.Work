
@inject NavigationManager NavManager

@code{

    [Parameter]
    public string? ReturnUrl { get; set; }

    protected override void OnInitialized()
    {
        try
        {
            NavManager.NavigateTo($"Identity/Account/Login?returnUrl=/{ReturnUrl}", true);
        }
        catch (Exception ex)
        {
            // throw;
        }
    }
}


